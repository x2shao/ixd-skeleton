<html>

<head>
	<title>Test Page</title>

	<link rel="stylesheet" href="/scripts/bootstrap/dist/css/bootstrap.min.css">
	<link rel="stylesheet" href="/scripts/bootstrap-material-design/dist/css/bootstrap-material-design.css">
	<link rel="stylesheet" href="/static/css/main.css">
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
</head>

<body class="bg-light">
	{{!-- Begin here (or replace this with any static code already written). --}}


	<nav class="navbar navbar-expand-lg navbar-light bg-light" style="max-height:100px">
		<div class="navbar-header">
			<a class="navbar-brand" href="#" style="text-align:center"><h3>hApp</h3></a>
			<a href="/home" class="float-left" href="#">
				<button type="button" class="btn btn-lg">
					<img src="/static/icons/home.svg" height="35"/>
				</button></a>
			<a class="float-right" href="/wip">
				<button type="button" class="btn btn-lg">
					<img src="/static/icons/controls.svg" height="35"/>
				</button></a>
		</div>
	</nav>

	<div class="container" style="height:calc(100vh - 200px - 5pt)"><div class="card" style="text-align:center; margin-top:0; margin-left:0; margin-right:0; height:100%">
		<div id="map" style="width:100%;height:400px;"></div>
		<div class="card-body">
			<h4 class="card-text text-left">{{name}}</h4>
			<h6 class="card-text text-left">{{distance}}</h6>
			<p class="card-text text-left">{{description}}</p>
		</div>
	</div></div>
	<div id="outer">
		<a><button class="btn inner" onclick="window.history.back()"><i class="fa fa-arrow-left"></i></button></a>
		&nbsp;&nbsp;&nbsp;
		<a href="/wip"><button class="btn inner btn-circle btn-xl"><i class="fa fa-share-alt"></i></button></a>
		&nbsp;&nbsp;&nbsp;
		<a href="/spider"><div class="inner"><button type="button" class="btn btn-secondary"><img src="/static/icons/ellipsis.svg" height="20"/></button></div></a>
	
	</div>

	

	<script src="/scripts/jquery/dist/jquery.js"></script>
	<script src="/scripts/bootstrap/dist/js/bootstrap.js"></script>
	<script src="/scripts/popper.js/dist/umd/popper.js"></script>
	<script src="/scripts/bootstrap-material-design/dist/js/bootstrap-material-design.js"></script>
	{{!-- In order for maps to work, the order must be: 1) gmaps, 2) main.js, 3) googleapis link with the key --}}
	<script src="/static/js/gmaps.js"></script>
	<script src="/static/js/main.js"></script>
	<script src="/static/js/mapView.js"></script>
	<script src="https://maps.googleapis.com/maps/api/js?key={{key}}"></script>
	 
</body>

</html>